package exceptions;

public class Demo1 {

	public static void main(String[] args) {
		int num1 = 10;
		int num2 = 5;
		int answer = 0;
		
		System.out.println("Going to Divide...");
		
		answer = num1 / num2;
		
		System.out.println("Answer is : " + answer);
		System.out.println("Division over...");
	}

}
