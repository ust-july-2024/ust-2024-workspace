package classesandobjects;

public abstract class Animal {
	private String name;
	
	public Animal(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	void breathe() {
		System.out.println(name + " breathes...");
	}
	
	abstract void mobility();
}
