package classesandobjects;

public class AnimalDemo {

	public static void main(String[] args) {
		Dog dog = new Dog("Roxie");
		dog.breathe();
		dog.mobility();
		dog.friendly();
		System.out.println("--------------");
		
		Snake snake = new Snake("Naga");
		snake.breathe();
		snake.mobility();
		snake.attacks();
	}

}
